{
    "version": 2,
    "buildCommand": "echo \"window.APP_CONFIG = { SPOTIFY_CLIENT_ID: '$SPOTIFY_CLIENT_ID' };\" > config.js",
    "outputDirectory": ".",
    "routes": [
      { "src": "/callback.html", "dest": "/callback.html" },
      { "src": "/stages/(.*)", "dest": "/stages/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }